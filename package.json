{
  "name": "imgsuki",
  "version": "1.0.0",
  "description": "A command line tool for manipulate images with Node.JS",
  "main": "index.js",
  "bin": "lib/suki.js",
  "scripts": {
    "start": "node lib/index.js",
    "build": "node_modules/.bin/pkg bin/suki.js --out-dir=snapshots",
    "example": "node ./lib resize C:\\Users\\Usuario\\repositories\\imgsuki\\test\\sample-images\\capella.jpg -o capella.out.jpg -w 150 -h 100",
    "test": "node_modules/.bin/mocha test"
  },
  "pkg": {
    "entrypoint": "bin",
    "scripts": "lib/**/*.js",
    "targets": [
      "node6",
      "node6-win-x64",
      "node6-win-x86",
      "node6-linux-x64",
      "node6-linux-x86",
      "node6-macos-x64",
      "node6-macos-x86"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiroDiaz/imgsuki.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SiroDiaz/imgsuki/issues"
  },
  "homepage": "https://github.com/SiroDiaz/imgsuki#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "commander": "^2.13.0",
    "file-type": "^7.5.0",
    "glob": "^7.1.2",
    "image-size": "^0.6.2",
    "read-chunk": "^2.1.0",
    "sharp": "^0.19.0",
    "upath": "^1.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "pkg": "^4.3.0"
  }
}
